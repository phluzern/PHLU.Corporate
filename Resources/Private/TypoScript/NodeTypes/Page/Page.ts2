prototype(PHLU.Corporate:Page) < prototype(PHLU.Neos.NodeTypes:Page) {


    breadcrumb = BreadcrumbMenu
    lead = TYPO3.TypoScript:Collection {
        collection = ${q(node).children('lead')}
        itemName = 'childNode'
        itemRenderer = TYPO3.TypoScript:Template {
            templatePath = 'resource://PHLU.Corporate/Private/Templates/NodeTypes/Content/Lead.html'
            node = ${childNode}
            test = 2
            text = ${q(node).property('text')}
        }
    }

}